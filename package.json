{
  "name": "co2-web-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/co2-web-components/co2-web-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test:unit": "stencil test --spec",
    "test:e2e": "stencil test --e2e",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "generate-css-vars": "sass --load-path ./node_modules --no-source-map ./src/variables.scss:./www/main.css"
  },
  "dependencies": {
    "@material/button": "^8.0.0",
    "@material/card": "^8.0.0",
    "@material/checkbox": "^8.0.0",
    "@material/icon-button": "^8.0.0",
    "@material/mwc-ripple": "^0.19.1",
    "@material/switch": "^8.0.0",
    "@material/textfield": "^8.0.0",
    "@material/theme": "^8.0.0",
    "@material/typography": "^8.0.0",
    "@stencil/core": "^2.0.1",
    "material-components-web": "^8.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/sass": "^1.4.0",
    "@types/jest": "^26.0.15",
    "@types/puppeteer": "^3.0.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "prettier": "^2.1.2",
    "puppeteer": "^5.4.1",
    "sass": "^1.29.0"
  }
}
