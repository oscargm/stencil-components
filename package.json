{
  "name": "co2-web-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/co2-web-components/co2-web-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "clean": "rimraf dist loader www storybook-static package-content",
    "build": "stencil build --docs",
    "serve": "stencil build --dev --watch --serve",
    "start": "stencil build --watch",
    "dev": "run-p start storybook",
    "storybook": "start-storybook -p 6006 -s www",
    "test:unit": "stencil test --spec",
    "test:e2e": "stencil test --e2e",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@material/button": "^8.0.0",
    "@material/checkbox": "^8.0.0",
    "@material/mwc-ripple": "^0.19.1",
    "@material/switch": "^8.0.0",
    "@material/textfield": "^8.0.0",
    "@material/theme": "^8.0.0",
    "@material/typography": "^8.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@open-wc/webpack-import-meta-loader": "^0.4.7",
    "@stencil/core": "^2.3.0",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.4.0",
    "@storybook/addon-a11y": "^6.1.11",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/addon-viewport": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/html": "^6.1.11",
    "@storybook/theming": "^6.1.11",
    "@types/autoprefixer": "^9.7.2",
    "@types/jest": "^26.0.15",
    "@types/puppeteer": "^3.0.2",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.1",
    "change-case": "^4.1.1",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "puppeteer": "^5.4.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
